<h1>New Event</h1>

<%= if @conn.assigns[:current_user] do %>
    <%= render "form.html", Map.put(assigns, :action, Routes.event_path(@conn, :create)) %>
<% else %>

        <div><h3>You Must be logged in to create an event.</h3></div>

<% end %>
    <span><%= link "Back", to: Routes.event_path(@conn, :index) %>


</span>
